{
  "logging": {
    "enabled": false,
    "debug": false,
    "info": false,
    "warning": false,
    "error": false,
    "suppressIconWarnings": true,
    "suppressQmlWarnings": true,
    "suppressHyprlandWarnings": true
  },
  "bar": {
    "showBackground": true,
    "bottom": false,
    "borderless": false,
    "topLeftIcon": "spark",
    "resources": {
      "alwaysShowSwap": true,
      "alwaysShowCpu": false
    },
    "workspaces": {
      "shown": 10,
      "alwaysShowNumbers": false,
      "showNumberDelay": 150
    }
  },
  "dock": {
    "height": 60,
    "hoverRegionHeight": 3,
    "pinnedOnStartup": false,
    "hoverToReveal": false,
    "enable": true,
    "pinnedApps": [
      "microsoft-edge-dev",
      "org.gnome.Nautilus",
      "vesktop",
      "cider",
      "steam-native",
      "lutris",
      "heroic",
      "obs",
      "org.gnome.Ptyxis"
    ]
  },
  "networking": {
    "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/123.0.0.0 Safari/537.36",
    "wifi": {
      "autoScan": true,
      "scanInterval": 30000,
      "showSignalStrength": true,
      "showSecurityType": true,
      "autoConnect": true,
      "rememberNetworks": true,
      "preferredNetworks": [],
      "showHiddenNetworks": false,
      "connection": {
        "timeout": 30000,
        "showPassword": false,
        "autoReconnect": true
      }
    }
  },
  "hacks": {
    "arbitraryRaceConditionDelay": 20
  }
} 